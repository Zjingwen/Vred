{
  "name": "vred",
  "version": "1.0.0",
  "description": "基于vue and 抄袭各种其他库的库",
  "main": "dist/vred.min.js",
  "scripts": {
    "watch": "webpack -w --config webpack.dev.config.js",
    "build": "npm run test && webpack --config webpack.pro.config.js",
    "server": "webpack-dev-server --open --config webpack.dev.config.js",
    "dev": "concurrently \"npm run watch\" \"npm run server\"",
    "line": "eslint --ext components/**/*.{js,vue}",
    "jest": "jest",
    "test": "npm run line && npm run jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "testMatch": [
      "**/tests/*.js"
    ],
    "moduleNameMapper": {
      "^@util/assist": "<rootDir>/components/util/assist.js"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.5",
    "markdown-loader": "^2.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "popper.js": "^1.14.3",
    "style-loader": "^0.21.0",
    "tooltip.js": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.16",
    "vue-eslint-parser": "^2.0.3",
    "vue-loader": "^15.1.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.0-beta.18",
    "babel-jest": "^23.0.1",
    "eslint": "^4.10.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^4.5.0",
    "jest": "^23.1.0",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "vue-jest": "^2.6.0"
  }
}
